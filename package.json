{
  "name": "mus-uc-devtools",
  "version": "0.1.0",
  "description": "A tool to develop userChrome CSS for Firefox using the Marionette protocol",
  "main": "index.js",
  "bin": {
    "mus-uc": "./bin/mus-uc.wasm"
  },
  "scripts": {
    "test": "node tests/headless-test.js",
    "test:vitest": "vitest",
    "test:vitest:watch": "vitest --watch",
    "test:vitest:firefox": "vitest --project firefox-chrome-context",
    "test:vitest:node": "vitest --project node-tests",
    "build": "cargo build --release --target wasm32-wasip1 && mkdir -p bin && cp target/wasm32-wasip1/release/mus-uc.wasm bin/",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "bin/mus-uc.wasm",
    "index.js",
    "README.md"
  ],
  "keywords": [
    "firefox",
    "userchrome",
    "css",
    "marionette",
    "devtools",
    "wasm",
    "wasi"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/f3liz-dev/mus-uc-devtools.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@puppeteer/browsers": "^2.4.1",
    "@vitest/runner": "^4.0.0-beta.18",
    "@vitest/utils": "^4.0.0-beta.18",
    "vitest": "^4.0.0-beta.18"
  }
}
